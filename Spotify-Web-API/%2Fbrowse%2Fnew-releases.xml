  <con:parameters/>
  <con:method name="get-new-releases" id="6b27f819-b67e-462a-a7a5-8396965e40bb" method="GET">
    <con:description>Get a list of new album releases featured in Spotify (shown, for example, on a Spotify player’s “Browse” tab).</con:description>
    <con:settings/>
    <con:parameters>
      <con:parameter>
        <con:name>country</con:name>
        <con:style>QUERY</con:style>
      </con:parameter>
      <con:parameter>
        <con:name>limit</con:name>
        <con:value>20</con:value>
        <con:style>QUERY</con:style>
        <con:default>20</con:default>
      </con:parameter>
      <con:parameter>
        <con:name>offset</con:name>
        <con:value>0</con:value>
        <con:style>QUERY</con:style>
        <con:default>0</con:default>
      </con:parameter>
    </con:parameters>
